{
  "name": "iot-lambda",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf \"dist/!(ffmpeg|node_modules|package.json|yarn.lock)\" && tsc",
    "deploy": "yarn build && yarn zip && yarn upload",
    "dev": "nodemon",
    "start": "node dist/index.js",
    "upload": "aws lambda update-function-code --function-name iot-lambda --zip-file fileb://dist.zip",
    "zip": "rm -f dist.zip && 7z a dist.zip ./dist/*"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "aws-sdk": "^2.1269.0"
  }
}
